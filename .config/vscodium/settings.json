{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.dragAndDrop": false,
  "editor.linkedEditing": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "expand",
  "editor.bracketPairColorization.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.wrapTabs": true,
  "workbench.startupEditor": "none",
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "security.workspace.trust.untrustedFiles": "newWindow",
  "window.restoreWindows": "none",
  "editor.formatOnSaveMode": "modifications",
  "git.mergeEditor": true,
  "editor.fontFamily": "'MesloLGS Nerd Font', 'MesloLGS Nerd Font Mono'",
  "terminal.integrated.fontFamily": "'MesloLGS Nerd Font', 'MesloLGS Nerd Font Mono'",
  "terminal.integrated.mouseWheelScrollSensitivity": 0.5,
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "window.commandCenter": true,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "vscode.json-language-features"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "eslint.validate": ["javascript", "typescript"],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "onHover",
  "update.showReleaseNotes": false,
  "window.density.editorTabHeight": "compact",
  "diffEditor.ignoreTrimWhitespace": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.sideBar.location": "right",
  "window.confirmSaveUntitledWorkspace": false,
  "explorer.compactFolders": false,
  "workbench.colorTheme": "GitHub Dark Default",
  "workbench.iconTheme": "material-icon-theme",
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.hidesExplorerArrows": true,
  "window.title": "${folderName}${separator}${remoteName}",
  "window.titleBarStyle": "custom",
  "remote.SSH.defaultExtensions": [
    "angular.ng-template",
    "dbaeumer.vscode-eslint",
    "eamodio.gitlens",
    "esbenp.prettier-vscode",
    "firsttris.vscode-jest-runner",
    "jeanp413.open-remote-ssh"
  ],
  "terminal.integrated.scrollback": 3000,
  "diffEditor.renderSideBySide": false,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}

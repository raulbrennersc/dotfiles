{
  "editor.formatOnSave": true,
  "editor.codeActionsOnSave": {
    "source.fixAll": "explicit"
  },
  "editor.dragAndDrop": false,
  "editor.linkedEditing": true,
  "editor.suggestSelection": "recentlyUsedByPrefix",
  "editor.minimap.renderCharacters": false,
  "editor.minimap.enabled": false,
  "explorer.confirmDelete": false,
  "git.autofetch": true,
  "editor.smoothScrolling": true,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "expand",
  "editor.bracketPairColorization.enabled": true,
  "typescript.updateImportsOnFileMove.enabled": "always",
  "workbench.editor.wrapTabs": true,
  "workbench.startupEditor": "none",
  "gitlens.advanced.messages": {
    "suppressGitMissingWarning": true
  },
  "terminal.external.windowsExec": "c:/Program Files/PowerShell/7/pwsh.exe",
  "terminal.integrated.scrollback": 3000,
  "security.workspace.trust.untrustedFiles": "newWindow",
  "window.restoreWindows": "none",
  "editor.formatOnSaveMode": "modifications",
  "git.mergeEditor": true,
  "terminal.integrated.defaultProfile.linux": "zsh",
  "editor.fontFamily": "'MesloLGS Nerd Font', 'MesloLGS Nerd Font Mono'",
  "terminal.integrated.fontFamily": "'MesloLGS Nerd Font', 'MesloLGS Nerd Font Mono'",
  "json.schemas": [],
  "terminal.integrated.mouseWheelScrollSensitivity": 0.5,
  "editor.cursorSmoothCaretAnimation": "on",
  "terminal.integrated.cursorBlinking": true,
  "terminal.integrated.cursorStyle": "underline",
  "terminal.integrated.persistentSessionReviveProcess": "never",
  "window.commandCenter": true,
"javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
    "[typescript]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "remote.SSH.remotePlatform": {
      "raspi": "linux"
    },
    "[json]": {
      "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
      "editor.defaultFormatter": "vscode.typescript-language-features"
  },
"eslint.validate": [
    "javascript",
    "typescript"
],
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/node_modules/*/**": true,
    "**/.hg/store/**": true
  },
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "onHover",
  "workbench.colorCustomizations": {
  "tree.indentGuidesStroke": "#929c95"
  },
  "material-icon-theme.activeIconPack": "nest",
  "material-icon-theme.folders.theme": "classic",
  "material-icon-theme.hidesExplorerArrows": true,
  "update.showReleaseNotes": false,
"window.density.editorTabHeight": "compact",
  "window.titleBarStyle": "custom",
"diffEditor.ignoreTrimWhitespace": false,
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.sideBar.location": "right",
  "window.confirmSaveUntitledWorkspace": false,
  "explorer.compactFolders": false
}